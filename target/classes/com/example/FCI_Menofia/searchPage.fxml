<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="438.0" prefWidth="839.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.FCI_Menofia.SearchPage">
   <children>
      <Label layoutX="526.0" layoutY="24.0" text="Search Page" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="backToMainPageButton" layoutX="31.0" layoutY="36.0" mnemonicParsing="false" onAction="#backToMainPage" text="&lt;- back to Main page" />
      <HBox alignment="CENTER" layoutY="100.0" prefHeight="100.0" prefWidth="526.0" spacing="15.0">
         <children>
            <Label text="Enter National id :">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="searchTextField" prefHeight="26.0" prefWidth="210.0" promptText="Search" />
            <Button fx:id="searchButton" mnemonicParsing="false" onAction="#searchForStudentInDatabaseUsingNationalId" text="Search" />
         </children>
      </HBox>
      <TableView fx:id="tabel" layoutY="200.0" prefHeight="200.0" prefWidth="839.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="75.0" text="name" />
          <TableColumn fx:id="level" prefWidth="75.0" text="level" />
            <TableColumn fx:id="department" prefWidth="75.0" text="department" />
            <TableColumn fx:id="section" prefWidth="75.0" text="section" />
            <TableColumn fx:id="jpa" prefWidth="75.0" text="jpa" />
            <TableColumn fx:id="course1" prefWidth="75.0" text="course1" />
            <TableColumn fx:id="course2" prefWidth="75.0" text="course2" />
            <TableColumn fx:id="course3" prefWidth="75.0" text="course3" />
            <TableColumn fx:id="course4" prefWidth="75.0" text="course4" />
            <TableColumn fx:id="course5" prefWidth="75.0" text="course5" />
            <TableColumn fx:id="course6" prefWidth="75.0" text="course6" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
